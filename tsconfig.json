{
  "compilerOptions": {
    "jsx": "react",
    "outDir": "./dist/",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "sourceMap": true,
    // "module": "es6",
    "target": "es6",
    "allowJs": true,
  },
  "types": [
    "react",
    "axios",
    "material-ui"
  ],
  "include": [
    "./src/client/**/*"
  ],
}